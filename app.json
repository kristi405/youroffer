{
  "expo": {
    "name": "youoffer",
    "scheme": "youoffer",
    "slug": "youoffer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-apple-authentication",
      ["expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(youoffer) to use your location."
        }
      ],
      "@react-native-google-signin/google-signin"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.offer.youoffer",
      "infoPlist": {
        "locationAlwaysAndWhenInUsePermission":"Allow $(youoffer) to use your location.",
        "locationWhenInUsePermission": "Allow $(youoffer) to use your location.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["com.googleusercontent.apps.431628664212-gfpbub2u5uj45nql72nd2hno30n8p4g1"]
          }
        ]
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDe_-maQXTeKOnHEKilBL5lcjKweuGYE-I"
        },
        "appleSignIn": {
          "clientId": "com.example.myapp.AppleSignIn",
          "scope": ["email", "name"]
        },
        "googleSignIn": {
          "expoClientId": "834107509512-4ml4fiue0sovdee82fuj67900vglpsdc.apps.googleusercontent.com"
        }
      }
    },
    "android": {
      "package": "com.offer.youoffer",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDe_-maQXTeKOnHEKilBL5lcjKweuGYE-I"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "googleSignIn": {
        "expoClientId": "431628664212-giaeh0eb4u6ptkmc2nahsa0mpbcobpab.apps.googleusercontent.com"
      }
    },
    "extra": {
      "eas": {
        "projectId": "bf189e94-81ad-4deb-9d3b-3de10662db11"
      }
    },
    "permissions": [
      "ACCESS_COARSE_LOCATION",
      "ACCESS_FINE_LOCATION"
    ],
    "owner": "kristina_gyk"
  }
}
